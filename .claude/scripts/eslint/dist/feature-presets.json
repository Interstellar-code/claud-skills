{
  "presets": {
    "subscription-form": {
      "description": "Subscription creation/editing forms (tabbed and modal forms)",
      "patterns": [
        "resources/js/pages/app/subscriptions/forms/**/*.{ts,tsx}",
        "resources/js/pages/app/subscriptions/forms/tabbed/**/*.{ts,tsx}",
        "resources/js/components/app/subscriptions/*form*.{ts,tsx}",
        "resources/js/types/**/*subscription*.ts"
      ],
      "excludes": [
        "**/*.test.{ts,tsx}",
        "**/*.spec.{ts,tsx}"
      ]
    },
    "subscription-dashboard": {
      "description": "Main subscriptions dashboard and listing",
      "patterns": [
        "resources/js/pages/app/subscriptions/subscriptions-content.tsx",
        "resources/js/pages/app/subscriptions/index.tsx",
        "resources/js/components/app/subscriptions/subscription-card.tsx",
